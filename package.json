    "nuxt": "3.17.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.3",
    "@nuxt/devtools": "2.4.1",
    "@stylistic/eslint-plugin": "4.4.0",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "22.15.21",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.27.0",
