    "@vercel/analytics": "1.5.0",
    "@vueuse/motion": "3.0.3",
    "nuxt": "3.17.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.0.0",
    "@nuxt/devtools": "2.6.2",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "24.1.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.32.0",
    "eslint-plugin-format": "1.0.1",
